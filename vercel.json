{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "build": {
        "env": {
            "NODE_VERSION": "18.x"
        }
    }
} 